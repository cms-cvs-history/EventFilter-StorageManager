process SM  = {

  source = FragmentInput { }

  module out = EventStreamFileWriter
  {
    // these are only valid for online storage manager
    string streamLabel = "A"
    int32 maxSize = 512

    untracked string SelectHLTOutput = "out4DQM"
  }

  module out2 = EventStreamFileWriter
  {
    // these are only valid for online storage manager
    string streamLabel = "B"
    int32 maxSize = 1024

    untracked PSet SelectEvents = { vstring SelectEvents={"*"}}
    untracked string SelectHLTOutput = "PhysicsOModule"
  }

  module out3 = EventStreamFileWriter
  {
    // these are only valid for online storage manager
    string streamLabel = "C"
    int32 maxSize = 200

    untracked PSet SelectEvents = { vstring SelectEvents={"DiMuon", "HighPT"}}
    untracked string SelectHLTOutput = "PhysicsOModule"
  }

  endpath e1 = { out }
  endpath e2 = { out2 }
  endpath e3 = { out3 }
}
